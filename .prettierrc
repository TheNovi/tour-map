{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"printWidth": 100,
	"pluginSearchDirs": ["."],
	"plugins": ["prettier-plugin-astro", "prettier-plugin-svelte"],
	"overrides": [{ "files": "*.astro", "options": { "parser": "astro" } }, { "files": "*.svelte", "options": { "parser": "svelte" } }]
}
